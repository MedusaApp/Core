{
    "name": "medusaapp/core",
    "type": "project",
    "description": "MEDUSA Core Application",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "GNU LGPL v3",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "6.0.*",
        "laravel/tinker": "^1.0",
        "medusaapp/personality": "dev-master",
        "pragmarx/countries-laravel": "^0.6.2",
        "silber/bouncer": "v1.0.0-rc.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core-personality",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../core-auth",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../core-views",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../core-registration",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../core-database",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../core-public-assets",
            "options": {
                "symlink": true
            }
        }
    ]
}
